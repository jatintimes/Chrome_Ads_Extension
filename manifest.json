{
    "name" : "Google Ads Extension - Ads Flame",
    "version": "0.0.1",
    "manifest_version": 3,
    "description" : "This is a extension that will provide you the information about the ads on the page by creating a table on the page.",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      },
    "web_accessible_resources": [{
        "resources": ["script.js"],
        "matches": ["<all_urls>"]
      }],
    "content_scripts" : [
        {
            "js" : ["init.js"],
            "css": ["style.css"],
            "matches" : ["<all_urls>"]
        }
    ]
}